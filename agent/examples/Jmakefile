	rules vacation

>PRIVLIB	/* Force metaconfig to ask for privlib location */

all::

MakeInstallDirectories($(PRIVLIB)/examples)
InstallMultipleDestFlags(install,$(FILES),$(PRIVLIB)/examples,-m 444)

